{
  "name": "nativescript-purchase",
  "version": "1.0.5",
  "description": "A NativeScript plugin for making in-app purchases",
  "main": "purchase",
  "typings": "nativescript-purchase.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "2.3.0",
      "android": "2.3.0"
    },
    "plugin": {
      "nan": "true",
      "pan": "true",
      "core3": "true",
      "category": "Utilities"
    }
  },

  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/nativescript-purchase"
  },

  "keywords": [
    "NativeScript",
    "iap",
    "in-app purchase",
    "in-app billing",
    "vending",
    "payment",
    "subscription",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "contributors": [
    "Sven Ziegler (https://github.com/svzi)"
  ],  
  "license": "Apache-2.0",

  "devDependencies": {
    "typescript": "2.1.4",
    "tslint": "4.3.1",
    "tns-core-modules-declarations": "2.3.1",
    "tns-platform-declarations": "2.3.0",

    "grunt": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-exec": "2.0.0"
  }

}
