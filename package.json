  },
  "devDependencies": {
    "tns-core-modules": "^4.1.0",
    "tns-platform-declarations": "4.1.0",
    "tslint": "^4.5.1",
    "typescript": "~2.7.0",
    "grunt": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-exec": "2.0.0"
  }
